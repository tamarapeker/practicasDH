<%- include('./header')%>

    <h1>Listado de peliculas</h1>
    <ul>
        <%for (let i=0 ; i < peliculas.length ; i++) {%>
           <a href="/movies/detail/<%=peliculas[i].id%>"> <li><%=peliculas[i].title%></li> </a>
        <% } %>
    </ul>

    <form action="/movies/search" method="POST">
        <input type="text" name="busqueda" placeholder="Buscar pelicula">
        <button type="submit">Buscar</button>
    </form>
</body>
</html>